<?xml version="1.0"?>
<policy type="and" name="flv_format_policy">
  <rule name="General/VideoCount is 1" value="VideoCount" tracktype="General" occurrence="*" operator="=">1</rule>
  <rule name="General/AudioCount is 1" value="AudioCount" tracktype="General" occurrence="*" operator="=">1</rule>
  <rule name="General/FileExtension is flv" value="FileExtension" tracktype="General" occurrence="*" operator="=">flv</rule>
  <rule name="General/Format is Flash Video" value="Format" tracktype="General" occurrence="*" operator="=">Flash Video</rule>
  <rule name="General/FrameRate is 29.970" value="FrameRate" tracktype="General" occurrence="*" operator="=">29.970</rule>
  <rule name="Video/Format is AVC" value="Format" tracktype="Video" occurrence="*" operator="=">AVC</rule>
  <rule name="Video/Format_Profile is High" value="Format_Profile" tracktype="Video" occurrence="*" operator="=">High</rule>
  <rule name="Video/Format_Level is 2" value="Format_Level" tracktype="Video" occurrence="*" operator="=">2</rule>
  <rule name="Video/Format_Settings_CABAC is Yes" value="Format_Settings_CABAC" tracktype="Video" occurrence="*" operator="=">Yes</rule>
  <rule name="Video/Format_Settings_RefFrames is 4" value="Format_Settings_RefFrames" tracktype="Video" occurrence="*" operator="=">4</rule>
  <rule name="Video/CodecID is 7" value="CodecID" tracktype="Video" occurrence="*" operator="=">7</rule>
  <rule name="Video/Width is 320" value="Width" tracktype="Video" occurrence="*" operator="=">320</rule>
  <rule name="Video/Height is 180" value="Height" tracktype="Video" occurrence="*" operator="=">180</rule>
  <rule name="Video/Stored_Height is 192" value="Stored_Height" tracktype="Video" occurrence="*" operator="=">192</rule>
  <rule name="Video/Sampled_Width is 320" value="Sampled_Width" tracktype="Video" occurrence="*" operator="=">320</rule>
  <rule name="Video/Sampled_Height is 180" value="Sampled_Height" tracktype="Video" occurrence="*" operator="=">180</rule>
  <rule name="Video/PixelAspectRatio is 1.000" value="PixelAspectRatio" tracktype="Video" occurrence="*" operator="=">1.000</rule>
  <rule name="Video/DisplayAspectRatio is 1.778" value="DisplayAspectRatio" tracktype="Video" occurrence="*" operator="=">1.778</rule>
  <rule name="Video/FrameRate_Mode is CFR" value="FrameRate_Mode" tracktype="Video" occurrence="*" operator="=">CFR</rule>
  <rule name="Video/FrameRate_Mode_Original is VFR" value="FrameRate_Mode_Original" tracktype="Video" occurrence="*" operator="=">VFR</rule>
  <rule name="Video/FrameRate is 29.970" value="FrameRate" tracktype="Video" occurrence="*" operator="=">29.970</rule>
  <rule name="Video/ColorSpace is YUV" value="ColorSpace" tracktype="Video" occurrence="*" operator="=">YUV</rule>
  <rule name="Video/ChromaSubsampling is 4:2:0" value="ChromaSubsampling" tracktype="Video" occurrence="*" operator="=">4:2:0</rule>
  <rule name="Video/BitDepth is 8" value="BitDepth" tracktype="Video" occurrence="*" operator="=">8</rule>
  <rule name="Video/ScanType is Progressive" value="ScanType" tracktype="Video" occurrence="*" operator="=">Progressive</rule>
  <rule name="Video/Encoded_Library_Name is x264" value="Encoded_Library_Name" tracktype="Video" occurrence="*" operator="=">x264</rule>
  <rule name="Audio/Format is AAC" value="Format" tracktype="Audio" occurrence="*" operator="=">AAC</rule>
  <rule name="Audio/Format_Profile is LC" value="Format_Profile" tracktype="Audio" occurrence="*" operator="=">LC</rule>
  <rule name="Audio/Format_Settings_SBR is No (Explicit)" value="Format_Settings_SBR" tracktype="Audio" occurrence="*" operator="=">No (Explicit)</rule>
  <rule name="Audio/CodecID is 10" value="CodecID" tracktype="Audio" occurrence="*" operator="=">10</rule>
  <rule name="Audio/Channels is 2" value="Channels" tracktype="Audio" occurrence="*" operator="=">2</rule>
  <rule name="Audio/ChannelPositions is Front: L R" value="ChannelPositions" tracktype="Audio" occurrence="*" operator="=">Front: L R</rule>
  <rule name="Audio/ChannelLayout is L R" value="ChannelLayout" tracktype="Audio" occurrence="*" operator="=">L R</rule>
  <rule name="Audio/SamplesPerFrame is 1024" value="SamplesPerFrame" tracktype="Audio" occurrence="*" operator="=">1024</rule>
  <rule name="Audio/SamplingRate is 48000" value="SamplingRate" tracktype="Audio" occurrence="*" operator="=">48000</rule>
  <rule name="Audio/FrameRate is 46.875" value="FrameRate" tracktype="Audio" occurrence="*" operator="=">46.875</rule>
  <rule name="Audio/Compression_Mode is Lossy" value="Compression_Mode" tracktype="Audio" occurrence="*" operator="=">Lossy</rule>
  <rule name="Audio/Delay_Source is Container" value="Delay_Source" tracktype="Audio" occurrence="*" operator="=">Container</rule>
</policy>
